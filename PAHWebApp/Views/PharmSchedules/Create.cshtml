@model PAHWebApp.Models.PharmSchedule
@inject PAHWebApp.Services.PahOptionsService pahOptionService

<h4>PharmSchedule</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ModifiedUserId" class="control-label"></label>
                <input asp-for="ModifiedUserId" class="form-control" />
                <span asp-validation-for="ModifiedUserId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ModifiedDateTime" class="control-label"></label>
                <input asp-for="ModifiedDateTime" class="form-control" />
                <span asp-validation-for="ModifiedDateTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MarketId" class="control-label"></label>
                <input asp-for="MarketId" class="form-control" />
                <span asp-validation-for="MarketId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleTypeId" class="control-label"></label>
                <input asp-for="ScheduleTypeId" class="form-control" />
                <span asp-validation-for="ScheduleTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleName" class="control-label"></label>
                <input asp-for="ScheduleName" class="form-control" />
                <span asp-validation-for="ScheduleName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleStartDate" class="control-label"></label>
                <input asp-for="ScheduleStartDate" class="form-control" />
                <span asp-validation-for="ScheduleStartDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleEndDate" class="control-label"></label>
                <input asp-for="ScheduleEndDate" class="form-control" />
                <span asp-validation-for="ScheduleEndDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FileName" class="control-label"></label>
                <input asp-for="FileName" class="form-control" />
                <span asp-validation-for="FileName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FilePath" class="control-label"></label>
                <input asp-for="FilePath" class="form-control" />
                <span asp-validation-for="FilePath" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsPreAdjudicated" /> @Html.DisplayNameFor(model => model.IsPreAdjudicated)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsScrubEnabled" /> @Html.DisplayNameFor(model => model.IsScrubEnabled)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleStatusId" class="control-label"></label>
                @Html.DropDownList("ScheduleStatusId", pahOptionService.ListScheduleStatuses().Select(s => new SelectListItem() { Text = s.ScheduleStatusName, Value = s.ScheduleStatusId.ToString()}))
                <span asp-validation-for="ScheduleStatusId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Count" class="control-label"></label>
                <input asp-for="Count" class="form-control" />
                <span asp-validation-for="Count" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SourceTypeId" class="control-label"></label>
                <input asp-for="SourceTypeId" class="form-control" />
                <span asp-validation-for="SourceTypeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsResubmit" /> @Html.DisplayNameFor(model => model.IsResubmit)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsForceVoid" /> @Html.DisplayNameFor(model => model.IsForceVoid)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <div class="checkbox">
                    <label>
                        <input asp-for="IsAutoGenerateFile" /> @Html.DisplayNameFor(model => model.IsAutoGenerateFile)
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label asp-for="ScheduleError" class="control-label"></label>
                <input asp-for="ScheduleError" class="form-control" />
                <span asp-validation-for="ScheduleError" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
